doctype html
html(lang= 'ru')
    head
        meta(charset='utf-8')
        title= 'table'
    body
        .page-table
            .wrapper
                table.page-content__table
                    thead
                        tr
                                th !{title1}
                                th !{title2}
                                th !{title3}
                                th !{title4}
                    tbody
                        each abit in abits
                            tr
                                td(colspan="4")
                                    .table__title
                                        img(src=require(`../img/abit/${abit.image}`) alt=abit.title)
                                        h2.title__header.title__header_full !{abit.title}

                            each content in abit.content
                                tr
                                    td(data-cell=title1)
                                        .left__text
                                            each names in content.arrayName
                                                div !{names.code}
                                                div !{names.name}
                                    td(data-cell=title2)
                                        .left__text
                                            each item in content.arrayItems
                                                div !{item.item}
                                    td(data-cell=title3)
                                        .center__block
                                            div
                                                each points in content.arrayPoints
                                                    div !{points.point}
                                    td(data-cell=title4)
                                        each item in content.arrayForm
                                            div !{item.form}


